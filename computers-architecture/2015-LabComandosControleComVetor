#Programa para multiplicar e dividir numeros inteiros por pontencias de 2
.data
V: .word 888 64 16 256 102 512 100 20 400
X: .word 1 1 1 1 1 1 1 1 1 #valores maiores que 100 posição = 1
tam: .word 9

.text
# contador interação
addu $t1, $0, $0
# ponteiros para V
la $t8, V
li $t3, 9
la $t2, X
li, $t7, 100
while1: beq $t3, $t1, exit
	lw $t4, 0($t8)
	bgt $t4, $t7, notZero
	sw $0, 0($t2) 
	notZero: 
	# incrementa contadores
	addi $t8, $t8, 4
	addi $t2, $t2, 4
	addi $t1, $t1, 1
	# verifica se Ã© necessÃ¡rio quebrar linha
	j while1
# encerra o programa com sucesso
exit: li $v0, 10
syscall
	
