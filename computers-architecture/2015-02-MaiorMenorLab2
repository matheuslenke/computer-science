#Lab2: Trocar o conteúdo de duas posiçoes de memória
.data
var1: .word 10
var2: .word 5

.text
#Solucao mais simples
#Observe as instruções de máquina geradas pelo montador
#lw $t0, var1
#lw $t1, var2
#sw $t0, var2
#sw $t1, var1

#Solucao menos evidente, economizando 3 instruções de máquina
#Usa apenas instruções básicas do ISA MIPS
lui $t7, 0x1001  #carrega o prefixo do segmento de dados em t7
lw $t0, 0($t7)
lw $t1, 4($t7)
sw $t1, 0($t7)
sw $t0, 4($t7)

#saída com sucesso  
addu $v0, $0, 10
syscall