## The Sum of 2 constant integer is displayed
.data
x1: .word 10
x2: .word 5
x3: .word -1

.text
#loads and store here are (extended) pseudo-instructions
#Observe the binary code in "real" MIPS instructions
lw $t0, x1
lw $t1, x2
add $t2, $t0, $t1
sw $t2, x3

# print sum result stored in $t2
li $v0, 1
move $a0, $t2
syscall
# execution terminated with success
li $v0, 10
syscall

